{
  "name": "statistics-dashboard",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "test:dev": "ng test  --code-coverage",
    "lint": "eslint --ext .js,.ts src/app --fix",
    "e2e": "ng e2e",
    "prettier": "prettier --write 'src/**/*.{ts,js,json,scss,html}' '*.{js,json}'",
    "dist": "ng build --build-optimizer=false --progress=false --prod && cp src/static/manifest.yml src/static/Staticfile src/static/nginx.conf dist",
    "lite": "test -d ./dist/statistics-dashboard && lite-server --baseDir=./dist/statistics-dashboard || echo '(run dist first)'"
  },
  "private": true,
  "dependencies": {
    "@amcharts/amcharts4": "^4.10.18",
    "@amcharts/amcharts4-geodata": "^4.1.20",
    "@angular/animations": "~10.0.11",
    "@angular/cdk": "^11.2.13",
    "@angular/common": "~10.0.11",
    "@angular/compiler": "^10.2.4",
    "@angular/core": "~10.0.11",
    "@angular/forms": "~10.0.11",
    "@angular/platform-browser": "~10.0.11",
    "@angular/platform-browser-dynamic": "~10.0.11",
    "@angular/router": "~10.0.11",
    "@swimlane/ngx-charts": "^16.0.0",
    "@swimlane/ngx-datatable": "^18.0.0",
    "html2canvas": "^1.0.0-rc.7",
    "ng-click-outside": "^7.0.1",
    "pdfmake": "^0.1.71",
    "rxjs": "^6.6.7",
    "tslib": "^2.2.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.7",
    "@angular/cli": "^10.2.1",
    "@angular/compiler-cli": "^10.2.4",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "^2.0.9",
    "@types/node": "^12.20.13",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "@typescript-eslint/types": "^4.25.0",
    "codelyzer": "^6.0.2",
    "eslint": "^7.27.0",
    "eslint-plugin-rxjs": "^2.1.7",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.6.0",
    "lite-server": "^2.6.1",
    "prettier": "^2.3.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "^3.9.9",
    "typescript-eslint": "0.0.1-alpha.0"
  }
}
